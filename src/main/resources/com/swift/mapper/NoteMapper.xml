<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.swift.mapper.NoteMapper">
    <resultMap id="noteResultMap" type="com.swift.domain.Note">
        <result column="card_id" property="cardId"/>
        <result column="card_title" property="cardTitle"/>
        <result column="card_body" property="cardBody"/>
        <result column="publication_date" property="publicationDate"/>
        <result column="visible" property="visible"/>
        <result column="expiration_date" property="expirationDate"/>
    </resultMap>
</mapper>